{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "apache/log4php": "2.3.0",
        "artesaos/seotools": "^0.19.0",
        "aws/aws-sdk-php": "^3.93",
        "barryvdh/laravel-dompdf": "^0.8.1",
        "chillerlan/php-qrcode": "^3.4",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "elibyy/tcpdf-laravel": "^8.1",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^5.0",
        "google/apiclient": "^2.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "kelvinmo/simplejwt": "0.3.*",
        "kreait/firebase-php": "^5.19",
        "kutia-software-company/larafirebase": "^1.3",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-ziparchive": "1.0",
        "league/iso3166": "^4.2",
        "livewire/livewire": "^2",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^8.0",
        "nesbot/carbon": "^2.66",
        "paragonie/random_compat": "^2.0",
        "paragonie/sodium_compat": "^1.6",
        "pear/numbers_words": "^0.18.2",
        "phpmailer/phpmailer": "^6.0",
        "phpoffice/phpexcel": "^1.8.0",
        "predis/predis": "^1.1",
        "razorpay/razorpay": "2.5.0",
        "sentry/sdk": "^3.1",
        "sentry/sentry-laravel": "^2.3",
        "silviolleite/laravelpwa": "~2.0.2",
        "smarty/smarty": "~3.1",
        "spatie/array-to-xml": "^2.15",
        "spatie/laravel-sitemap": "^5.8",
        "stripe/stripe-php": "^7.100",
        "swipezdev/short-url-package-creator": "dev-master",
        "symfony/security-csrf": "^5.0",
        "web-token/jwt-encryption": "^2.0",
        "webklex/laravel-pdfmerger": "*",
        "yajra/laravel-datatables-oracle": "^9.19"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "facade/ignition": "^2.3.6",
        "kitloong/laravel-migrations-generator": "^6.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
            {
            "type": "composer",
            "url": "https://packagist.org"
            },
            { "packagist": false },
            { "type": "vcs", "url": "git@github.com:swipezdev/short-url-package-creator.git" }
    ]

}
